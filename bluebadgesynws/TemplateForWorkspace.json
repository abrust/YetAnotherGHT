						"cell_type": "code",
						"source": [
							"spark.Sql(\"INSERT INTO cities VALUES ('Seattle', 730400), ('San Francisco', 881549), ('Beijing', 21540000), ('Bangalore', 10540000)\")"
						],
						"outputs": [],
						"execution_count": 2
					},

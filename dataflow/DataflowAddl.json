{
	"name": "DataflowAddl",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "MoviesCSVDSAddl",
						"type": "DatasetReference"
					},
					"name": "MoviesCSVSourceAddl"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "ADLSServiceAddl",
						"type": "LinkedServiceReference"
					},
					"name": "MoviesDeltaSinkAddl"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          movies as integer,",
				"          Title as string,",
				"          genresgenregenre as string,",
				"          YEAR as short,",
				"          Rating as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> MoviesCSVSourceAddl",
				"MoviesCSVSourceAddl sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'adftutorial',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> MoviesDeltaSinkAddl"
			]
		}
	}
}